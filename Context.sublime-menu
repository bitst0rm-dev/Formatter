                    {
                        "caption": "Shell",
                        "children": [
                            {
                                "caption": "Run current file",
                                "command": "run_repl",
                                "args": {
                                    "uid": "shell",
                                    "cmd": {
                                        "windows": [["cmd"], "/k", "${{file}}"],
                                        "linux": [["bash"], "-i", "${{file}}"],
                                        "osx": [["bash"], "-i", "${{file}}"]
                                    },
                                    "syntax": "Packages/ShellScript/Bash.sublime-syntax",
                                    "filter_echo": true,
                                    "encoding": {
                                        "windows": "${{locale_encoding}}",
                                        "linux": "utf-8",
                                        "osx": "utf-8"
                                    },
                                    "env": {
                                        "NO_COLOR": "1"
                                    }
                                }
                            },
