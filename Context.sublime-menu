[
    {
        "caption": "Formatter",
        "id": "formatter",
        "children": [
            {
                "caption": "☰   Quick Options",
                "command": "quick_options"
            },
            {
                "caption": "❯_  Interactive REPL",
                "children": [
                    {
                        "caption": "CoffeeScript",
                        "children": [
                            {
                                "caption": "Run current file",
                                "command": "run_repl",
                                "args": {
                                    "uid": "coffee",
                                    "cmd": [["coffee"], "-i", "-r", "${{file}}"],
                                    "syntax": "Packages/JavaScript/JavaScript.sublime-syntax",
                                    "encoding": "utf-8",
                                    "env": {
                                        "NO_COLOR": "1",
                                        "NODE_DISABLE_COLORS": "1"
                                    }
                                }
                            },
                            {
                                "caption": "Terminal",
                                "command": "run_repl",
                                "args": {
                                    "uid": "coffee",
                                    "cmd": [["coffee"], "-i"],
                                    "syntax": "Packages/JavaScript/JavaScript.sublime-syntax",
                                    "encoding": "utf-8",
                                    "env": {
                                        "NO_COLOR": "1",
                                        "NODE_DISABLE_COLORS": "1"
                                    }
                                }
                            }
                        ]
                    },
                    {
                        "caption": "Erlang",
                        "children": [
                            {
                                "caption": "Terminal",
                                "command": "run_repl",
                                "args": {
                                    "uid": "erlang",
                                    "cmd": [["erl"]],
                                    "syntax": "Packages/Erlang/Erlang.sublime-syntax",
                                    "encoding": "utf-8",
                                    "env": {
                                        "NO_COLOR": "1"
                                    }
                                }
                            }
                        ]
                    },
