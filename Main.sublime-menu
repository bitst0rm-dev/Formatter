    {
        "caption": "Preferences",
        "id": "preferences",
        "mnemonic": "n",
        "children": [
            {
                "caption": "Package Settings",
                "id": "package-settings",
                "mnemonic": "P",
                "children": [
                    {
                        "caption": "Formatter",
                        "mnemonic": "F",
                        "children": [
                            {
                                "caption": "Settings",
                                "mnemonic": "S",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/Formatter/Formatter.sublime-settings",
                                    "default": "// Do NOT edit anything in the left-hand pane.\n// Pick up items you need, but make sure to maintain the structure.\n{\n\t$0\n}\n"
                                }
                            },
                            {
                                "caption": "Open Config Folders",
                                "mnemonic": "O",
                                "command": "open_config_folders"
                            },
                            {
                                "caption": "-"
                            },
                            {
                                "caption": "Example Key Bindings",
                                "mnemonic": "E",
                                "command": "open_file",
                                "args": {
                                    "file": "${packages}/Formatter/Example.sublime-keymap"
                                }
                            },
                            {
                                "caption": "Key Bindings – User",
                                "mnemonic": "K",
                                "command": "open_file",
                                "args": {
                                    "file": "${packages}/User/Default (${platform}).sublime-keymap"
                                }
                            },
                            {
                                "caption": "-"
                            },
                            {
                                "caption": "Version Info",
                                "mnemonic": "V",
                                "command": "show_version"
                            }
                        ]
