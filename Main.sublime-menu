[
    {
        "caption": "Tools",
        "id": "tools",
        "mnemonic": "T",
        "children": [
            {
                "caption": "Formatter",
                "mnemonic": "F",
                "id": "formatter",
                "children": [
                    {
                        "caption": "☰   Quick Options",
                        "mnemonic": "Q",
                        "command": "quick_options"
                    },
                    {
                        "caption": "❯_  Interactive REPL",
                        "mnemonic": "I",
                        "children": [
                            {
                                "caption": "Node.js",
                                "mnemonic": "N",
                                "children": [
                                    {
                                        "caption": "Run current file",
                                        "mnemonic": "R",
                                        "command": "run_repl",
                                        "args": {
                                            "uid": "node",
                                            "cmd": [["node"], "-i", "-r", "${file}"],
                                            "syntax": "Packages/JavaScript/JavaScript.sublime-syntax"
                                        }
                                    },
                                    {
                                        "caption": "Terminal",
                                        "mnemonic": "T",
                                        "command": "run_repl",
                                        "args": {
                                            "uid": "node",
                                            "cmd": [["node"], "-i"],
                                            "syntax": "Packages/JavaScript/JavaScript.sublime-syntax"
                                        }
                                    }
                                ]
                            },
                            {
                                "caption": "PHP",
                                "mnemonic": "P",
                                "children": [
                                    {
                                        "caption": "Run current file",
                                        "mnemonic": "R",
                                        "command": "run_repl",
                                        "args": {
                                            "uid": "php",
                                            "cmd": [["php"], "-a", "-d", "auto_prepend_file=${file}"],
                                            "syntax": "Packages/PHP/PHP.sublime-syntax"
                                        }
                                    },
                                    {
                                        "caption": "Terminal",
                                        "mnemonic": "T",
                                        "command": "run_repl",
                                        "args": {
                                            "uid": "php",
                                            "cmd": [["php"], "-a"],
                                            "syntax": "Packages/PHP/PHP.sublime-syntax"
                                        }
                                    }
                                ]
                            },
                            {
                                "caption": "Python",
                                "mnemonic": "P",
                                "children": [
                                    {
                                        "caption": "Run current file",
                                        "mnemonic": "R",
                                        "command": "run_repl",
                                        "args": {
                                            "uid": "python",
                                            "cmd": [["python3", "python"], "-i", "-B", "${file}"],
                                            "syntax": "Packages/Python/Python.sublime-syntax"
                                        }
                                    },
                                    {
                                        "caption": "Terminal",
                                        "mnemonic": "T",
                                        "command": "run_repl",
                                        "args": {
                                            "uid": "python",
                                            "cmd": [["python3", "python"], "-i", "-B"],
                                            "syntax": "Packages/Python/Python.sublime-syntax"
                                        }
                                    }
                                ]
                            },
                            {
                                "caption": "Shell",
                                "mnemonic": "S",
                                "children": [
                                    {
                                        "caption": "Run current file",
                                        "mnemonic": "R",
                                        "command": "run_repl",
                                        "args": {
                                            "uid": "shell",
                                            "cmd": {
                                                "windows": [["cmd"], "/k", "${file}"],
                                                "linux": [["bash"], "-i", "${file}"],
                                                "osx": [["bash"], "-i", "${file}"]
                                            },
                                            "syntax": "Packages/ShellScript/Bash.sublime-syntax"
                                        }
                                    },
                                    {
                                        "caption": "Terminal",
                                        "mnemonic": "T",
                                        "command": "run_repl",
                                        "args": {
                                            "uid": "shell",
                                            "cmd": {
                                                "windows": [["cmd"]],
                                                "linux": [["bash"], "-i"],
                                                "osx": [["bash"], "-i"]
                                            },
                                            "syntax": "Packages/ShellScript/Bash.sublime-syntax"
                                        }
                                    }
                                ]
                            }
                        ]
